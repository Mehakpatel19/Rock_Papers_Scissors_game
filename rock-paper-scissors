import random
rock=0
paper=1
scissor=2
comp_choice=random.randint(0,2)
user_choice=int(input("Enter 0 for rock,1 for paper,2 for scissor:"))
if user_choice==0 and comp_choice==1:
    print("You lose")#rock-paper
elif user_choice==0 and comp_choice==2:
    print("You win")#rock-scissor
elif user_choice==1 and comp_choice==2:
    print("You lose")#paper-scissors
elif user_choice==1 and comp_choice==0:
    print("You win")#paper-rock
elif user_choice==2 and comp_choice==0:
    print("You lose")#scissor-rock
elif user_choice==2 and comp_choice==1:
    print("You win")#scissor-paper
else:
    print("Tie.....play again")
